<div class="jumbotron">
  <div class="row">
    <div class="col-xs-12">
      <h2 class="text-center">Edit User</h1>
    </div>

    <div class="col-md-6 col-md-offset-3 col-xs-12">
      <%= form_for current_user do |f| %>
        <%= render "form", f: f %>
        <%= f.submit 'Register', class: 'btn btn-primary' %>
      <% end %>
    </div>
  </div>
</div>
