<div class="jumbotron">
  <h2>Contacts</h2>

  <table>
    <thead>
      <tr>
        <th>Name</th>
        <th>Phone</th>
        <th colspan="3">Options</th>
      </tr>
    </thead>
    <tbody>
      <% @contacts.each do |contact| %>
        <tr>
          <td><%= contact.name %></td>
          <td><%= contact.phone %></td>
          <td><%= link_to 'Show' %></td>
          <td><%= link_to 'Edit' %></td>
          <td><%= link_to 'Destroy' %></td>
        </tr>
      <% end  %>
    </tbody>
  </table>
  <br>
  <%= link_to 'New Contact', new_contact_path %>

</div>
